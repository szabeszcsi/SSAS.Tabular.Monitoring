<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="xSessionName" value="StatStream"/>
    <add key="ssasConnectionString" value="Provider=MSOLAP;Data Source=TSZDELL2015\SQL2017;Integrated Security=SSPI;"/>
